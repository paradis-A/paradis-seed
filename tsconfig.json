{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": [
            "es2020",
            "es6"
        ],
        "declaration": true,
        "outDir": "dist",
        "rootDir": "lib",
        "strict": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "removeComments": true,
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "lib/model",
        "dev_*.ts"
    ]
}
